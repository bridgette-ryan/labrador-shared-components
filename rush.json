{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.155.1",
  "pnpmVersion": "10.13.1",
  "nodeSupportedVersionRange": ">=18.20.3 <19.0.0 || >=20.14.0 <25.0.0",
  "gitPolicy": {},
  "repository": {
    "url": "https://github.com/bridgette-ryan/labrador-shared-components",
    "defaultBranch": "main"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": [],
    "preRushx": [],
    "postRushx": []
  },
  "projectFolderMinDepth": 1,
  "projectFolderMaxDepth": 5,
  "variants": [],
  "allowedProjectTags": [
    "shared-component",
    "atom-component",
    "molecule-component",
    "layout-component",
    "organism-component",
    "non-published-template"
  ],
  "projects": [
    {
      "packageName": "@labrador-sgd/component-base",
      "projectFolder": "src/base",
      "tags": [
        "non-published-template"
      ],
      "shouldPublish": false
    },
    {
      "packageName": "@labrador-sgd/column",
      "projectFolder": "src/layouts/column",
      "tags": [
        "layout-component"
      ],
      "shouldPublish": true
    },
    {
      "packageName": "@labrador-sgd/row",
      "projectFolder": "src/layouts/row",
      "tags": [
        "layout-component"
      ],
      "shouldPublish": true
    },
    {
      "packageName": "@labrador-sgd/smart-grid",
      "projectFolder": "src/layouts/smart-grid",
      "tags": [
        "layout-component"
      ],
      "shouldPublish": true
    },
    {
      "packageName": "@labrador-sgd/button",
      "projectFolder": "src/atoms/button",
      "tags": [
        "atom-component"
      ],
      "shouldPublish": true
    }
  ]
}